{
    "projectId": "69496060003d273d4a8c",
    "endpoint": "https://fra.cloud.appwrite.io/v1",
    "functions": [
        {
            "$id": "pbapi",
            "name": "Plant-Based API",
            "runtime": "python-3.12",
            "specification": "s-0.5vcpu-512mb",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/adapters/rest/appwrite_functions.py",
            "commands": "pip install -r requirements.txt",
            "path": "",
            "variables": {
                "DOPPLER_CONFIG": "${{ secrets.DOPPLER_CONFIG || 'prod' }}",
                "DOPPLER_PROJECT": "${{ secrets.DOPPLER_PROJECT || 'pbapi' }}",
                "PBAPI_DOPPLER_PROD_TOKEN": "${{ secrets.PBAPI_DOPPLER_PROD_TOKEN }}"
            }
        }
    ]
}